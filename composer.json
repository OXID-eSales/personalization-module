{
    "name": "oxid-esales/personalization-module",
    "description": "This is the Personalization module for the OXID eShop.",
    "type": "oxideshop-module",
    "keywords": ["oxid", "modules", "eShop"],
    "homepage": "https://www.oxid-esales.com",
    "license": [
        "proprietary"
    ],
    "extra": {
        "oxideshop": {
            "blacklist-filter": [
                "documentation/**/*.*",
                "composer.json",
                "README.md",
                "LICENSE",
                "CHANGELOG.md"
            ],
            "target-directory": "oe/personalization"
        }
    },
    "require": {
        "php": ">=7.0",
        "webmozart/path-util": "^2.3",
        "gargron/fileupload": "^1.4.5",
        "symfony/filesystem": "^3.0",
        "econda/tracking": "^3.0.1",
        "league/csv": "^9.1.4"
    },
    "bin": [
        "bin/oe-personalization-data-feed"
    ],
    "autoload": {
        "psr-4": {
            "OxidEsales\\PersonalizationModule\\": "../../../source/modules/oe/personalization"
        }
    }
}
